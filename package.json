{
  "name": "vitepress_blog",
  "version": "0.0.1",
  "description": "冯思学的博客",
  "main": "index.js",
  "scripts": {
    "docs:dev": "vitepress dev .",
    "docs:build": "vitepress build .",
    "dockerfile:build": "docker build -t vitepress_blog .",
    "dockerfile:run": "docker run -p 443:443 -p 80:80 --name blog -ti vitepress_blog",
    "dockerfile:dev": "npm run dockerfile:build && npm run dockerfile:run",
    "eslint:lint": "eslint . --fix",
    "prepare": "husky install && husky add .husky/pre-commit 'pnpm eslint:lint&&git add .'"
  },
  "repository": {
    "type": "git",
    "url": "https://e.coding.net/abliger/abliger/vitepress_blog.git"
  },
  "keywords": [
    "blog",
    "vitepress"
  ],
  "author": "fengsixue",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1 || ^5.0.0",
    "eslint-plugin-vue": "latest",
    "husky": "^7.0.4",
    "vite": "^2.7.1",
    "vitepress": "^0.20.2"
  }
}
